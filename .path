echo "2: .path....."
# * ~/.path can be used to extend `$PATH` and $manpath
# Put $HOME/{sbin,bin} first
# export PATH=/home7/tvctopin/bin:/home7/tvctopin/sbin

for linuxbrew_bin in \
  /home7/tvctopin/.linuxbrew/bin \
  /home7/tvctopin/.linuxbrew/opt/coreutils/libexec/gnubin \
  /home7/tvctopin/.linuxbrew/opt/binutils/libexec/gnubin \
  /home7/tvctopin/.linuxbrew/opt/findutils/libexec/gnubin \
  /home7/tvctopin/.linuxbrew/opt/libtool/libexec/gnubin \
  /home7/tvctopin/.linuxbrew/opt/gnu-sed/libexec/gnubin \
  /home7/tvctopin/.linuxbrew/opt/gnu-units/libexec/gnubin
do
  [[ -e $linuxbrew_bin ]] && export PATH=$linuxbrew_bin:$PATH
done

for ext_bin in \
  $HOME/ruby/gems/bin \
  $HOME/perl/bin \
  $HOME/perl5/bin \
  $HOME/bin \
  $HOME/sbin
do
  [[ -e $ext_bin ]] && export PATH=$ext_bin:$PATH
done

export HOMEBREW_TEMP=/home7/tvctopin/var/tmp
export HOMEBREW_CACHE=/home7/tvctopin/.linuxbrew/Library/Caches/Homebrew
# I don't like fun
export HOMEBREW_NO_EMOJI='1'
# Gimme errors
export HOMEBREW_DEVELOPER='1'

if [[ -n `which brew` ]]; then
  # Find a Homebrew-built Python
  python_bin=$(brew --cellar python)/*/bin
  python_bin=`echo $python_bin`
  [[ -e $python_bin ]] && export PATH=$python_bin:$PATH

  [[ -e $(brew --prefix)/share/python ]] && export PATH=$(brew --prefix)/share/python:$PATH

  # Find a Homebrew-built Ruby
  ruby_bin=$(brew --prefix ruby)/bin
  ruby_bin=`echo $ruby_bin`
  [[ -e $ruby_bin ]] && export PATH=$ruby_bin:$PATH

  # Add Homebrew completions and homebrew sourced completions
  source $(brew --repo)/Library/Contributions/brew_bash_completion.sh
  for comp in \
    $(brew --prefix)/etc/bash_completion \
    $(brew --prefix)/etc/bash_completion.d/git-completion.bash
  do
    [[ -e $comp ]] && source $comp
done
fi

