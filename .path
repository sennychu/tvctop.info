export LD_LIBRARY_PATH=/home7/tvctopin/.linuxbrew/lib


for linuxbrew_manpath in \
  /home7/tvctopin/.linuxbrew/opt/coreutils/libexec/gnuman \
  /home7/tvctopin/.linuxbrew/opt/binutils/libexec/gnuman \
  /home7/tvctopin/.linuxbrew/opt/findutils/libexec/gnuman \
  /home7/tvctopin/.linuxbrew/opt/libtool/libexec/gnuman \
  /home7/tvctopin/.linuxbrew/opt/gnu-sed/libexec/gnuman \
  /home7/tvctopin/.linuxbrew/opt/gnu-units/libexec/gnuman 
do
  [[ -e $linuxbrew_manpath ]] && export MANPATH=$linuxbrew_manpath:$MANPATH
done


